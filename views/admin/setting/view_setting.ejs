<%- include('../../partials/header.ejs') %>

<!-- Page Wrapper -->
<div id="wrapper">
  <!-- Sidebar -->
  <%- include('../../partials/sidebar.ejs') %>
  <!-- End of Sidebar -->

  <!-- Content Wrapper -->
  <div id="content-wrapper" class="d-flex flex-column">
    <!-- Main Content -->
    <div id="content">
      <!-- Topbar -->
      <%- include('../../partials/navbar.ejs') %>
      <!-- End of Topbar -->

      <!-- Begin Page Content -->
      <div class="container-fluid">
        <%- include ../../partials/message.ejs %>

        <section id="content">
          <div class="container-fluid">
            <div class="row mb-2 align-items-center">
              <div class="col-sm-6">
                <h1 class="title-dashboard">Setting</h1>
              </div>
              <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                  <li class="breadcrumb-item"><a href="/">Dashboard</a></li>
                  <li class="breadcrumb-item active">Setting</li>
                </ol>
              </div>
            </div>
          </div>
          <!-- /.container-fluid -->
        </section>

        <!-- Main content -->
        <section class="content">
          <div class="container-fluid">
            <form action="/setting/edit/2?_method=PUT">
              <div class="mb-3">
                <label for="exampleInputEmail1" class="form-label">Name</label>
                <input
                  type="name"
                  value="<%= name %>"
                  class="form-control"
                  name="name"
                  id="name"
                />
              </div>
            </form>
            <div class="row">
              <div class="col-lg-6">
                <form action="/setting/edit/62772ed88e95ab077ed764a3?_method=PUT" method="POST">
                  <div class="mb-3">
                    <label for="exampleInputPassword1" class="form-label"
                      >Nama Vegetable</label
                    >
                    <input
                      type="text"
                      value="<%= vegetable1.name %>"
                      class="form-control"
                      name="name"
                      id="name"
                    />
                  </div>
                  <div class="mb-3">
                    <label for="exampleInputPassword1" class="form-label"
                      >Jumlah Vegetable</label
                    >
                    <input
                      type="number"
                      value="<%= vegetable1.sum %>"
                      class="form-control"
                      name="sum"
                      id="sum"
                    />
                  </div>
                  <button type="submit" class="btn btn-primary">Submit</button>  
                </form>
              </div>
              <div class="col-lg-6">
                <form action="/setting/edit/62787d4f9f1c9d93d46815cd?_method=PUT" method="POST">
                  <div class="mb-3">
                    <label for="exampleInputPassword1" class="form-label"
                      >Nama Vegetable</label
                    >
                    <input
                      type="text"
                      value="<%= vegetable2.name %>"
                      class="form-control"
                      name="name"
                      id="name"
                    />
                  </div>
                  <div class="mb-3">
                    <label for="exampleInputPassword1" class="form-label"
                      >Jumlah Vegetable</label
                    >
                    <input
                      type="number"
                      value="<%= vegetable2.sum %>"
                      class="form-control"
                      name="sum"
                      id="sum"
                    />
                  </div>
                  <button type="submit" class="btn btn-primary">Submit</button>  
                </form>
              </div>
            </div>

            <!-- /.row -->
          </div>
          <!-- /.container-fluid -->
        </section>
      </div>
      <!-- /.container-fluid -->
    </div>
    <!-- End of Main Content -->

    <!-- Footer -->
    <%- include('../../partials/footer.ejs') %>
    <!-- End of Footer -->
  </div>
  <!-- End of Content Wrapper -->
</div>
<!-- End of Page Wrapper -->

<!-- Scroll to Top Button-->
<a class="scroll-to-top rounded" href="#page-top">
  <i class="fas fa-angle-up"></i>
</a>

<!-- Logout Modal-->
<div
  class="modal fade"
  id="logoutModal"
  tabindex="-1"
  role="dialog"
  aria-labelledby="exampleModalLabel"
  aria-hidden="true"
>
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Ready to Leave?</h5>
        <button
          class="close"
          type="button"
          data-dismiss="modal"
          aria-label="Close"
        >
          <span aria-hidden="true">Ã—</span>
        </button>
      </div>
      <div class="modal-body">
        Select "Logout" below if you are ready to end your current session.
      </div>
      <div class="modal-footer">
        <button class="btn btn-secondary" type="button" data-dismiss="modal">
          Cancel
        </button>
        <a class="btn btn-primary" href="login.html">Logout</a>
      </div>
    </div>
  </div>
</div>

<%- include('../../partials/js.ejs') %> <%- include('../../partials/gauges.ejs')
%>
