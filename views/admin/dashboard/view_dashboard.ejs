<%- include('../../partials/header.ejs') %>

<!-- Page Wrapper -->
<div id="wrapper">
  <!-- Sidebar -->
  <%- include('../../partials/sidebar.ejs') %>
  <!-- End of Sidebar -->

  <!-- Content Wrapper -->
  <div id="content-wrapper" class="d-flex flex-column">
    <!-- Main Content -->
    <div id="content">
      <!-- Topbar -->
      <%- include('../../partials/navbar.ejs') %>
      <!-- End of Topbar -->

      <!-- Begin Page Content -->
      <div class="container-fluid">
        <%- include ../../partials/message.ejs %>
        <!-- Page Heading -->
        <section id="content">
          <div class="row mb-2 align-items-center pr-4">
            <div class="col-sm-6 mr-auto">
              <h1 class="title-dashboard">Dashboard</h1>
              <h1 class="subtitle-dashboard">Manage Our IOT System</h1>
            </div>
            <div class="ml-lg-auto ml-4">
              <div class="d-flex user-logged nav-item dropdown no-arrow">
                <a
                  href="#"
                  role="button"
                  id="dropdownMenuLink"
                  data-bs-toggle="dropdown"
                  aria-expanded="false"
                >
                <div class="row align-items-center text-center">
                  <img
                    src="/images/ic-profile.svg"
                    alt=""
                    class="image-profile"
                  />
                  <h1 class="user-name"><%= name %></h1>
                </div>
                  <ul
                    class="dropdown-menu"
                    aria-labelledby="dropdownMenuLink"
                    style="right: 0; left: auto"
                  >
                    <li>
                      <a href="" class="dropdown-item"
                        >My Profile</a
                      >
                    </li>         
                    <li>
                      <a
                        href="/logout"
                        class="dropdown-item"
                        onclick=""
                        >Sign Out</a
                      >
                    
                    </li>
                  </ul>
                </a>
              </div>

            </div>
          </div>
          <!-- /.container-fluid -->
        </section>
        <!-- Earnings (Monthly) Card Example -->
        <div class="row mt-lg-4">
          <div class="col-xl-3 col-md-6 mb-4">
            <div class="card border-left-primary py-2">
              <div class="card-body">
                <div class="row align-items-center">
                  <div class="ml-2"></div>
                  <img
                    src="/images/icon-user.png"
                    alt=""
                    height="50"
                    width="50"
                  />
                  <div class="font-weight-medium color-pallete1 ml-2">
                    Current <br />User
                  </div>
                </div>

                <div class="row no-gutters align-items-center">
                  <div class="col mr-2">
                    <div class="text-xs text-gray-500 text-uppercase my-2">
                      Total
                    </div>
                    <div class="h5 mb-0 font-weight-medium color-pallete1">
                      <%= count.user %>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="col-xl-3 col-md-6 mb-4">
            <div class="card border-left-success py-2">
              <div class="card-body">
                <div class="row align-items-center">
                  <div class="ml-2"></div>
                  <img
                    src="/images/icon-vegetable.png"
                    alt=""
                    height="50"
                    width="50"
                  />
                  <div class="font-weight-medium color-pallete1 ml-2">
                    Current <br />Vegetable
                  </div>
                </div>

                <div class="row no-gutters align-items-center">
                  <div class="col mr-2">
                    <div class="text-xs text-gray-500 text-uppercase my-2">
                      Total
                    </div>
                    <div class="h5 mb-0 font-weight-medium color-pallete1">
                      <%= vegetable1.sum + vegetable2.sum %>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="col-xl-3 col-md-6 mb-4">
            <div class="card border-left-warning border py-2">
              <div class="card-body">
                <div class="row align-items-center">
                  <div class="ml-2"></div>
                  <img
                    src="/images/icon-weather.png"
                    alt=""
                    height="50"
                    width="50"
                  />
                  <div class="font-weight-medium color-pallete1 ml-2">
                    Weather <br />Monitoring
                  </div>
                </div>

                <div class="row no-gutters align-items-center">
                  <div class="col mr-2">
                    <div class="text-xs text-gray-500 text-uppercase my-2">
                      Total
                    </div>
                    <div class="h5 mb-0 font-weight-medium color-pallete1">
                      Hujan
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="row monitoring mb-lg-1 mb-5">
          <div class="col-lg-6">
            <h1 class="title-monitoring">Data Monitoring</h1>
            <div class="row">
              <div class="col-6">
                <div class="card">
