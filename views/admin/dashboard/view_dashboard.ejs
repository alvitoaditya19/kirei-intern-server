                    <img
                      src="/images/icon-lamp2.svg"
                      alt=""
                      height="60"
                      width="60"
                      class="mb-2"
                    />
                    <h1 class="title">Lamp 1 OFF</h1>
                  </button>
                </div>
                <% } else if(lampu.lampu1 === 'ON') { %>
                <div class="">
                  <button class="btn-lamp-on btn">
                    <img
                      src="/images/icon-lamp1.svg"
