                    <p class="card-text hidden"></p>
                    <div class="row align-items-center">
                      <div class="">
                        <img
                          src="/images/ic-temp.svg"
                          alt=""
                          class="card-icon"
                        />
                      </div>
                      <div class="ml-lg-3">
                        <h1 class="result" id="suhuCelcius"></h1>
                        <h1 class="info">celcius</h1>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <div class="col-6">
                <div class="card">
                  <div class="card-body-monitoring">
                    <h1 class="card-title">Humidity</h1>
                    <p class="card-text hidden"></p>
                    <div class="row align-items-center">
                      <div class="">
                        <img
                          src="/images/ic-temp.svg"
                          alt=""
                          class="card-icon"
                        />
                      </div>
                      <div class="ml-lg-3">
