                  <button class="btn-lamp-on btn">
                    <img
                      src="/images/icon-lamp1.svg"
                      alt=""
                      height="60"
                      width="60"
                      class="mb-2"
                    />
                    <h1 class="title">Lamp 2 ON</h1>
                  </button>
                </div>
                <% } %>
              </form>
              <form
                action="/dashboard/pump1/62430c4028e1eef562010284?_method=PUT"
