<script>
    setInterval(getData,1000)


function getData(){
    $.ajax({
        type: "GET",
        headers: {
            "Content-Type": "application/json"
        },
        url: "http://localhost:4000/api/v1/lampu/lampu",
        success: function (data) {
            $("#avatar").attr("src", data.avatar_url);
        
            $("#lampuu").text(data.data[0].lampu1);
            const date = new Date();
              document.getElementById("timeSerial").innerHTML = date.toLocaleTimeString();
        }
    });
}


setInterval(myTimer, 1000);

function myTimer() {
  const date = new Date();
  document.getElementById("demo").innerHTML = date.toLocaleTimeString();
}
</script>
